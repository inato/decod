{
  "name": "decod",
  "version": "3.2.0",
  "description": "Decode unknwon typescript values",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanwalj/decod.git"
  },
  "keywords": [
    "Typescript",
    "safe",
    "decode",
    "unknown",
    "values"
  ],
  "author": "Jordan Van Walleghem <j.vanwall@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vanwalj/decod/issues"
  },
  "homepage": "https://github.com/vanwalj/decod#readme",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1"
  },
  "sideEffects": false
}
